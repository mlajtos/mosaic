{
  "name": "mosaic-browser",
  "productName": "Mosaic",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Web browser that can display multiple web pages at the same time in one shared window.",
  "author": {
    "name": "Milan Lajto≈°",
    "email": "milan.lajtos@me.com",
    "url": "https://github.com/mlajtos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlajtos/mosaic.git"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "build": "electron-builder -wlm",
    "dist": "yarn compile && yarn build",
    "release": "build"
  },
  "build": {
    "appId": "com.github.mlajtos.mosaic",
    "buildVersion": "1",
    "files": [
      "build/.*png"
    ],
    "publish": "github",
    "win": {
      "icon": "build/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "category": "Network",
      "icon": "build/icon.png",
      "synopsis": "Web browser that can display multiple web pages at the same time in one shared window.",
      "executableName": "mosaic",
      "target": [
        "deb",
        "rpm",
        "pacman",
        "tar.gz"
      ]
    }
  },
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@cliqz/adblocker-electron": "^1.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@react-hook/merged-ref": "^1.1.1",
    "@types/is-url": "^1.2.28",
    "@types/lodash": "^4.14.155",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/recoil": "^0.0.0",
    "@types/uuid": "^7.0.4",
    "babel-loader": "^8.1.0",
    "cross-fetch": "^3.0.4",
    "electron": "8.2.5",
    "electron-builder": "^22.7.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "golden-layout": "1.5.9",
    "is-url": "^1.2.4",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "^0.0.8",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.3",
    "uuid": "7.0.3",
    "webpack": "~4.43.0"
  }
}
